function autocapitaliseFieldDropoff() {
    autoRemoveFormErrors();
    var inputDropoff = document.getElementById("input-dropoff");
    //new google.maps.places.Autocomplete(inputDropoff);

    if (inputDropoff.value != null && inputDropoff.value != "") {
        inputDropoff.className = "input-field-upper delivery-location";
        $('.postcode_delivery_error').html('');
    }
    else if (inputDropoff.value == "") {
        inputDropoff.className = "input-field-quote delivery-location";
    }
}

function myFunction(value) {
    $('.vehicle_id_error').html('');
    id = value;
    $('.remove-active').removeClass('active');
    $('.account-heading' + id).addClass('active');
    if (inputPickup.value != "" && inputDropoff.value != "" && autoUpdatePrice) {
        this.submitQuote();
    }

    if (this.detailObj != undefined && autoUpdatePrice) {
        this.submitQuote();
    }

    // Instantiate an new XHR Object
    const xhr = new XMLHttpRequest();
    xhr.open("GET","https://alpha-api.deliveryapp.com/api/v1/vehicles/" + id, true);
    xhr.onload = function () {
        if (this.status === 200) {
            detailObj = JSON.parse(this.responseText);
        }
        else {
            console.log("File not found");
        }
    }
    xhr.send();
}

function autoRemoveQuoteErrors() {
    if (inputPickup.value == "") {
        inputContainerPickup.className += " input-error";
        errorLabelPickup.className = "";
    }
    if (inputDropoff.value == "") {
        inputContainerDropoff.className += " input-error";
        errorLabelDropoff.className = "";
    }
    if (inputEmail.value == "") {
        inputContainerEmail.className += " input-error";
        errorLabelEmail.className = "";

    }
    if (inputContact.value == "") {
        inputContainerContact.className += " input-error";
        errorLabelContact.className = "";

    }
    if (inputName.value == "") {
        inputContainerName.className += " input-error";
        errorLabelName.className = "";

    }
}

function autoRemoveFormErrors() {
    if (inputFirstName.value != "") {
        inputContainerFirstName.className = "input-container col-md-12";
        errorLabelFirstName.className = "hidden";
    }
    //    if (inputPickup.value != "") {
    //  inputContainerPickup.className = "input-container col-md-12";
    //    errorLabelPickup.className = "hidden";
    //}
    if (inputDropoff.value != "") {
        inputContainerDropoff.className = "input-container col-md-12";
        errorLabelDropoff.className = "hidden";
    }
    if (inputEmail.value != "" && inputEmail.value.includes('@') && inputEmail.value.includes('.')) {
        inputContainerEmail.className = "input-container col-md-12";
        errorLabelEmail.className = "hidden";
    }
    if (inputContact.value != "" && inputContact.value.length >= 10 && isNumber(inputContact.value)) {
        inputContainerContact.className = "input-container col-md-12";
        errorLabelContact.className = "hidden";
    }
    if (inputName.value != "") {
        inputContainerName.className = "input-container col-md-12";
        errorLabelName.className = "hidden";
    }
    if (checkboxInput.checked == true) {
        errorLabelCheckbox.className = "hidden";
        checkboxInput.style = "";
    }
}

function formErrors() {
    if (inputPickup.value == "") {
        inputContainerPickup.className += " input-error";
        errorLabelPickup.className = "";
        errorSpanPickup.innerHTML = " The pick-up postcode is required";
    }
    else {
        inputContainerPickup.className = "input-container col-md-12";
        errorLabelPickup.className = "hidden";
    }
    if (inputDropoff.value == "") {
        inputContainerDropoff.className += " input-error";
        errorLabelDropoff.className = "";
        errorSpanDropoff.innerHTML = " The drop-off postcode is required";
    }
    else {
        inputContainerDropoff.className = "input-container col-md-12";
        errorLabelDropoff.className = "hidden";
    }
    if (inputEmail.value == "") {
        inputContainerEmail.className += " input-error";
        errorLabelEmail.className = "";
    }
    else if (!inputEmail.value.includes('@') || !inputEmail.value.includes('.')) {
        inputContainerEmail.className += " input-error";
        errorLabelEmail.className = "";
    }
    else {
        inputContainerEmail.className = "input-container col-md-12";
        errorLabelEmail.className = "hidden";
    }
    if (inputContact.value == "") {
        inputContainerContact.className += " input-error";
        errorLabelContact.className = "";

    }
    else if (inputContact.value.length < 10 && isNumber(inputContact.value)) {
        console.log(inputContact.value.length);
        inputContainerContact.className += " input-error";
        errorLabelContact.className = "";
        errorSpanContact.innerHTML = "Invalid mobile format";
    }
    else {
        inputContainerContact.className = "input-container col-md-12";
        errorLabelContact.className = "hidden";
    }
    if (inputName.value == "") {
        inputContainerName.className += " input-error";
        errorLabelName.className = "";
    }
    else {
        inputContainerName.className = "input-container col-md-12";
        errorLabelName.className = "hidden";
    }
    if (checkboxInput.checked == false) {
        errorLabelCheckbox.className = "visible";
        checkboxInput.style = "border: 1px solid red;";
    }
    else {
        errorLabelCheckbox.className = "hidden";
        checkboxInput.style = "";
    }
}

function invalidPostcodeError() {
    if (inputEmail.value.includes('@') && inputEmail.value.includes('.')) {
        errorSpanDropoff.innerHTML = " Please double check postcode";
        errorSpanPickup.innerHTML = " Please double check postcode";
        inputContainerPickup.className += " input-error";
        errorLabelPickup.className = "";
        inputContainerDropoff.className += " input-error";
        errorLabelDropoff.className = "";
    }
}

function submitQuote() {
    window.dataLayer = window.dataLayer || [];
    window.dataLayer.push({
        'event': 'getAQuote'
    });

    formErrors();
    if (checkboxInput.checked && isNumber(inputContact.value) && inputContact.value.length >= 10) {
        let params = {
            postcode_delivery: inputDropoff.value,
            postcode_pickup: inputPickup.value,
            name: inputName.value,
            email: inputEmail.value,
            phone: inputContact.value,
            url: 'http://www.deliveryapp.com/quote/'
        }
        if (!radioYes.checked) {
            params.name = "NotProvided";
        }


        let xhr = new XMLHttpRequest();
        xhr.open("POST", "https://alpha-api.deliveryapp.com/api/wordpress/quote", true);
        xhr.setRequestHeader('Content-type', 'application/json');
		
        xhr.onload = function () {
            containsCongestion = false;
            try {
                data = JSON.parse(this.responseText);
                console.log(data);
            }
            catch {
                console.log("error");
            }
            if (this.status == 200) {
                if (inputEmail.value.includes('@') && inputEmail.value.includes('.')) {
                    vehiclePrices = data;
                    let congestionPrice = parseInt(vehiclePrices.items[0].congestion);
                    if(congestionPrice > 0){
                        console.log('Contains congestion');
                        containsCongestion = true;
                        navigateToVehiclePrices();
                        autoUpdatePrice = true;
                    }
                    else{
                        navigateToVehiclePrices();
                    }
                }
            }
            else if (this.status === 500) {
                invalidPostcodeError();
            }
            else if (this.status === 422) {
                console.log("422 error");
            }
        }
        xhr.send(JSON.stringify(params));
    }
}

function resetButton() {
    $('.pick-location').val('');
    $('.delivery-location').val('');
    $('.remove-active').removeClass('active');
    $('.per-mile-price').html("£" + "0.00");
    autoUpdatePrice = false;
    inputPickup.value = "";
    inputDropoff.value = "";
    myFunction(null);
    autocapitaliseFieldDropoff();
    autocapitaliseFieldPickup();
    buttonBook.className = "hidden";
    buttonCallback.className = "hidden";
}

function acceptTerms() {
    inputContainerPickup.className += " input-error";
    inputContainerDropoff.className += " input-error";
    inputContainerEmail.className += " input-error";
    inputContainerContact.className += " input-error";
    inputContainerName.className += " input-error";
    errorLabel.className = "";
}


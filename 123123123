function togglePasswordView() {
    if (password.type == "password") {
        password.type = "";
        passwordImg.src = "https://unpkg.com/ionicons@5.5.1/dist/svg/eye-outline.svg";
    }
    else {
        password.type = "password";
        passwordImg.src = "https://unpkg.com/ionicons@5.5.1/dist/svg/eye-off-outline.svg";
    }
}

function togglePasswordConfirmView() {
    if (passwordConfirm.type == "password") {
        passwordConfirm.type = "";
        passwordConfirmImg.src = "https://unpkg.com/ionicons@5.5.1/dist/svg/eye-outline.svg";
    }
    else {
        passwordConfirm.type = "password";
        passwordConfirmImg.src = "https://unpkg.com/ionicons@5.5.1/dist/svg/eye-off-outline.svg";
    }
}

function submitRegistration() {
    if (contactNumber.value.length >= 10 && password.value.length >= 8 && checkboxInput.checked && firstName.value != "" && lastName.value != "" && emailAddress.value != "" && contactNumber.value != "" && password.value != "" && passwordConfirm.value != "" && password.value == passwordConfirm.value && emailAddress.value.includes('@')) {
        const xhr = new XMLHttpRequest();
        errorMessage = "";
        errorMessageEmail = "";
        errorMessageNumber = "";
        errorMessagePassword = "";
        displayErrors();
        let number = "+44" + contactNumber.value;
        let data = {
            "name": firstName.value,
            "surname": lastName.value,
            "mobile_number": number,
            "email": emailAddress.value,
            "password": password.value,
            "password_confirmation": passwordConfirm.value,
            "terms": 1
        }
        xhr.open("POST","https://alpha-api.deliveryapp.com/api/user/register", true);
        xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
        xhr.onload = function () {
            if (this.readyState === XMLHttpRequest.DONE && this.status === 200) {
                errorMessage = "";
                errorMessageEmail = "";
                errorMessageNumber = "";
                errorMessagePassword = "";
                switchView();
            }
            else {
                let object = JSON.parse(this.responseText);
                errorMessage = "";
                errorMessageEmail = "";
                errorMessageNumber = "";
                errorMessagePassword = "";
                for (key in object) {
                    if (object[key][0] != undefined) {
                        if (object[key][0].includes("email")) {
                            if (object[key][0] != "The email has already been taken.") {
                                errorMessageEmail = object[key][0];
                            }
                            errorMessageEmail = "Email already in use";
                        }
                        if (object[key][0].includes("number")) {
                            if (object[key][0] != "The mobile number format is invalid.") {
                                errorMessageNumber = object[key][0];
                            }
                            errorMessageNumber = "Invalid number format";
                        }
                        if (object[key][0].includes("password")) {
                            errorMessagePassword = object[key][0];
                        }
                    }
                }
                displayErrors();
            }
        }
        xhr.send(JSON.stringify(data));
    }
    else {
        displayErrors();
    }
}

function autoRemoveErrors() {
    if (firstName.value == "") {
    }
    else {
        errorFirstName.className = "hidden-text";
        firstName.className = "input-field";
    }
    if (lastName.value == "") {
    }
    else {
        errorLastName.className = "hidden-text";
        lastName.className = "input-field";
    }
    if (contactNumber.value == "") {
    }
    else {
        errorContactNumber.className = "hidden-text";
        contactNumber.className = "input-field";
    }
    if (emailAddress.value == "") {
    }
    else if (!emailAddress.value.includes('@') || emailAddress.value.includes(' ')) {
    }
    else {
        errorEmailAddress.className = "hidden-text";
        emailAddress.className = "input-field";
    }
    if (password.value == "") {
    }
    else {
        errorPassword.className = "hidden-text";
        password.className = "input-field";
    }
    if (passwordConfirm.value == "") {
    }
    else {
        passwordConfirm.className = "input-field";
        errorPasswordConfirm.className = "hidden-text";
    }
    if (password.value != passwordConfirm.value) {
    }
    else {
        errorPasswordConfirm.className = "hidden-text";
        passwordConfirm.className = "input-field";
    }
    if (!checkboxInput.checked) {
    }
    else {
        errorCheckbox.className = "hidden-text";
    }
}


function displayErrors() {
    if (firstName.value == "") {
        errorFirstName.className = "error-label";
        firstName.className = "input-field-error";
    }
    else {
        errorFirstName.className = "hidden-text";
        firstName.className = "input-field";
    }
    if (lastName.value == "") {
        errorLastName.className = "error-label";
        lastName.className = "input-field-error";
    }
    else {
        errorLastName.className = "hidden-text";
        lastName.className = "input-field";
    }
    if (contactNumber.value == "") {
        errorContactNumber.className = "error-label";
        contactNumber.className = "input-field-error";
        errorContactNumber.innerHTML = "Contact number required";
    }
    else if (contactNumber.value.length < 10) {
        errorContactNumber.className = "error-label";
        contactNumber.className = "input-field-error";
        errorContactNumber.innerHTML = "Invalid number format";
    }
    else if (errorMessageNumber != "") {
        errorContactNumber.className = "error-label";
        contactNumber.className = "input-field-error";
        errorContactNumber.innerHTML = errorMessageNumber;
    }
    else {
        errorContactNumber.className = "hidden-text";
        contactNumber.className = "input-field";
    }
    if (emailAddress.value == "") {
        errorEmailAddress.className = "error-label";
        errorEmailAddress.innerHTML = "Email address required";
        emailAddress.className = "input-field-error";
    }
    else if (!emailAddress.value.includes('@') || emailAddress.value.includes(' ')) {
        errorEmailAddress.className = "error-label";
        errorEmailAddress.innerHTML = "Invalid email format";
        emailAddress.className = "input-field-error";
    }
    else if (errorMessageEmail != "") {
        errorEmailAddress.className = "error-label";
        errorEmailAddress.innerHTML = errorMessageEmail;
        emailAddress.className = "input-field-error";
    }
    else {
        errorEmailAddress.className = "hidden-text";
        emailAddress.className = "input-field";
    }
    if (password.value == "") {
        errorPassword.className = "error-label";
        password.className = "input-field-error";
        errorPassword.innerHTML = "Please confirm your password";
    }
    else if (password.value.length < 8) {
        errorPassword.className = "error-label";
        password.className = "input-field-error";
        errorPassword.innerHTML = "Minimum 8 characters required";
    }
    else {
        errorPassword.className = "hidden-text";
        password.className = "input-field";
        // 
    }
    if (passwordConfirm.value == "") {
        passwordConfirm.className = "input-field-error";
        errorPasswordConfirm.className = "error-label";
        errorPasswordConfirm.textContent = "Please confirm your password";
    }
    else {
        passwordConfirm.className = "input-field";
        errorPasswordConfirm.className = "hidden-text";
    }
    if (password.value != passwordConfirm.value) {
        errorPasswordConfirm.className = "error-label";
        errorPasswordConfirm.textContent = "Passwords do not match";
        passwordConfirm.className = "input-field-error";
    }
    else {
        errorPasswordConfirm.className = "hidden-text";
        passwordConfirm.className = "input-field";
    }
    if (!checkboxInput.checked) {
        errorCheckbox.className = "error-label";
    }
    else {
        errorCheckbox.className = "hidden-text";
    }
}

</script>

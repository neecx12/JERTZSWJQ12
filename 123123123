function togglePriceDisplay() {
    if (priceToggle.checked) {
        carLabel.innerHTML = "£" + vehiclePrices.items[0].price_vat;
        SVLabel.innerHTML = "£" + vehiclePrices.items[1].price_vat;
        MVLabel.innerHTML = "£" + vehiclePrices.items[2].price_vat;
        LVLabel.innerHTML = "£" + vehiclePrices.items[3].price_vat;
        XLVLabel.innerHTML = "£" + vehiclePrices.items[4].price_vat;
        LutonLabel.innerHTML = "£" + vehiclePrices.items[5].price_vat;
    }
    else {
        carLabel.innerHTML = "£" + vehiclePrices.items[0].price_without_vat_with_toll;
        SVLabel.innerHTML = "£" + vehiclePrices.items[1].price_without_vat_with_toll;
        MVLabel.innerHTML = "£" + vehiclePrices.items[2].price_without_vat_with_toll;
        LVLabel.innerHTML = "£" + vehiclePrices.items[3].price_without_vat_with_toll;
        XLVLabel.innerHTML = "£" + vehiclePrices.items[4].price_without_vat_with_toll;
        LutonLabel.innerHTML = "£" + vehiclePrices.items[5].price_without_vat_with_toll;
    }
    if(containsCongestion){
        carLabel.innerHTML += "*";
        SVLabel.innerHTML += "*";
        MVLabel.innerHTML += "*";
        LVLabel.innerHTML += "*";
        XLVLabel.innerHTML += "*";
        LutonLabel.innerHTML += "*";
        congestionHint.className = "font-style-small";
    }
    else{
        congestionHint.className = "hidden";
    }

}


function autoRemovePasswordError() {
    if (passwordInput.value.length >= 8) {
        errorLabelPassword.style = "display: none;";
        inputContainerPassword.style = "background: #EDEDED;border-radius:5px; ";
    }
}

function registerAccountErrors() {
    //if (inputSurname.value == "") {
        //errorLabelSurname.className = "";
        //inputContainerSurname.className += " input-error";
    //}
    //else {
        //inputContainerSurname.className = "input-container col-md-12";
        //errorLabelSurname.className = "hidden";
    //}
    if (inputFirstName.value == "") {
        errorLabelFirstName.className = "";
        inputContainerFirstName.className += " input-error";
    }
    else {
        inputContainerFirstName.className = "input-container col-md-12";
        errorLabelFirstName.className = "hidden";
    }
}

function registerAccount() {
    registerAccountErrors();

    errorLabelPassword.style = "display: none;";
    inputContainerPassword.style = "background: #EDEDED;border-radius:5px; ";

    if (passwordInput.value.length < 8) {
        errorLabelPassword.style = "";
        inputContainerPassword.style = "background: #EDEDED;border-radius:5px; border: 2px solid red;";
    }

    if (passwordInput.value.length >= 8) {
        xhr.open("POST", "https://alpha-api.deliveryapp.com/api/user/register", true);
        xhr.setRequestHeader('Content-type', 'application/json');
        xhr.setRequestHeader('Accept', 'application/json');

        xhr.onload = function () {
            if (this.status === 200) {
                switchView();
            } else if (this.status === 422) {
                let errorMsg = JSON.parse(this.responseText);

                errorLabelPassword.style = "";
                passwordErrorSpan.innerHTML = errorMsg.message;

                $.each(errorMsg.errors, function( index, value ) {
                    passwordErrorSpan.innerHTML =  value ;
                });
            } else if (this.status === 206 || this.status === 302) {
                errorLabelPassword.style = "";
                passwordErrorSpan.innerHTML = "You have already registered an account!";
            } else {
                errorLabelPassword.style = "";
                passwordErrorSpan.innerHTML = "Error, please try again.";
            }
        }
        let phoneNo = inputContact.value;
        if (inputContact.value[0] == "0") {
            phoneNo = phoneNo.substring(1);
        }
        let data = {
            "email": inputEmail.value,
            "password": passwordInput.value,
            "role": "customer", // WE NEED ADD GUEST FOR REGISTER VALIDATION
            "terms": 1,
            "details" : {
                "full_name": inputFirstName.value,
                "contact_number": phoneNo,
                "is_business": false
            }
        }

        xhr.send(JSON.stringify(data));
    }

    console.log("s");
    return;
}

function togglePasswordDisplay() {
    if (passwordHidden) {
        passwordImgShow.className = "hidden";
        passwordImgHide.className = "password-img";
        passwordInput.type = "";
    }
    else {
        passwordImgHide.className = "hidden";
        passwordImgShow.className = "password-img";
        passwordInput.type = "password";
    }
    passwordHidden = !passwordHidden;
}

function createAccount() {
    if (stepTwoActive) {
        navigateToVehiclePrices();
        createAccountDiv.style = "display: none;";
        accountCreationDiv.className = "account-creation-div";
        frameStepTwo.style = "";
        accountCreationHint.innerHTML = "Set a password to create your account and request your delivery today.";
    }
}

function radioToggle() {
    if (radioYes.checked) {
        companyNameDiv.style = "";
        nameLabel.innerHTML = "Company Name *";
        nameSpanError.innerHTML = " Please enter your company name";
        priceToggle.checked = false;
    }
    else {
        companyNameDiv.style = "display: none;";
        nameLabel.innerHTML = "Full Name *";
        nameSpanError.innerHTML = " Please enter your full name";
        priceToggle.checked = true;
    }
}


        inputPickup.className = "input-field-upper pickup-location";
    }
    else if (inputPickup.value == "") {
        inputPickup.className = "input-field-quote pickup-location";
    }
}

function isNumber(str) {
    var pattern = /^\d+$/;
    return pattern.test(str);  // returns a boolean
}



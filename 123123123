                        <img id="passwordImgHide" onclick="togglePasswordDisplay()" class="hidden" src="https://www.deliveryapp.com/wp-content/themes/deliveryapp/images/show_pwd_icon.svg" data-pagespeed-url-hash="909133922" onload="pagespeed.CriticalImages.checkImageForCriticality(this);">
                    </div>
                    <label class="password-hint">Min 8 characters</label>
                    <span class="postcode_pickup_error password-error" style="color: red; position: relative;top: 0px;left: 0px;"><label class="error-label" id="errorLabelPassword" style="display: none;"><img class="error-img" id="error-svg" src="https://www.deliveryapp.com/wp-content/themes/deliveryapp/images/bx_bxs-error.svg" data-pagespeed-url-hash="2925039386" onload="pagespeed.CriticalImages.checkImageForCriticality(this);"><span style="margin-left: 15px;" class="error-span" id="passwordErrorSpan"> Password field
                                is required</span></label></span>
                </div>

                <div style="float: left !important; padding: 0px 0px 0px 16px ; margin-top: 30px; margin-left: 0px;" class="input-item-left col-md-12 input-item-mini">

                    <div class="wildcard-style col-md-12">
                        <p style="color: transparent !important;" class="input-field-label no-padding input-field-mobile">
                            .
                        </p>
                    </div>

                    <div class="input-container col-md-12 btn-mini label-mini" id="inputContainerPickup" style="background: #EDEDED;border-radius:10px; padding: 0px;">
                        <input class="input-field-quote pick-location btn-mini" id="input-pickup" type="button" onclick="registerAccount()" value="Submit" name="usrnm" style="background: #223039;border: 1px solid white;color:#fff; font-size: 18px;font-weight: 600;">
                        <span class="postcode_pickup_error" style="color: red;"><label class="hidden" id="errorLabelPickup"><img class="error-img" id="error-svg" src="https://www.deliveryapp.com/wp-content/themes/deliveryapp/images/bx_bxs-error.svg" data-pagespeed-url-hash="2925039386" onload="pagespeed.CriticalImages.checkImageForCriticality(this);"><span class="error-span"> The pick-up postcode is required</span></label></span>
